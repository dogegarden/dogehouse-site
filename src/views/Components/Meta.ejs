    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta name="robots" content="all, follow" />
    <meta
      name="description"
      content="<%= process.env.WEBSITE.description; %>"
    />
    <meta name="keywords" content="<%= process.env.WEBSITE_KEYWORDS; %>" />
    <meta name="author" content="<%= process.env.WEBSITE_AUTHOR; %>" />
    <meta name="subject" content="<%= process.env.WEBSITE_SUBJECT; %>" />
    <meta name="revised" content="<%= process.env.WEBSITE_REVISION_DATE; %>" />
    <meta name="url" content="<%= process.env.WEBSITE_URL; %>" />
    <meta property="og:locale" content="en_GB" />
    <!--
        <meta property="og:locale:alternate" content="fr_FR" />
        <meta property="og:locale:alternate" content="es_ES" />
    -->
    <link rel="canonical" href="<% process.env.WEBSITE_URL; %>" />
    <meta http-equiv="Expires" content="0" />
    <meta http-equiv="Pragma" content="no-cache" />
    <meta http-equiv="Cache-Control" content="no-cache" />
    <meta http-equiv="imagetoolbar" content="no" />
    <meta http-equiv="x-dns-prefetch-control" content="off" />
    <meta name="twitter:card" content="<%= process.env.WEBSITE_SUMMARY; %>" />
    <meta name="twitter:site" content="@TWITTER_ID" />
    <meta name="twitter:title" content="<%= process.env.WEBSITE_TITLE; %>" />
    <meta
      name="twitter:description"
      content="<%= process.env.WEBSITE_AUTHOR; %>"
    />
    <meta name="twitter:creator" content="<%= process.env.WEBSITE_AUTHOR; %>" />
    <meta name="twitter:image" content="<%= process.env.WEBSITE_IMAGE; %>" />
    <meta property="og:title" content="<%= process.env.WEBSITE_TITLE; %>" />
    <meta property="og:type" content="website" />
    <meta property="og:url" content="<%= process.env.WEBSITE_URL; %>" />
    <meta property="og:image" content="<%= process.env.WEBSITE_IMAGE; %>" />
    <meta
      property="og:description"
      content="<%= process.env.WEBSITE_DESCRIPTION; %>"
    />
    <meta property="og:site_name" content="<%= process.env.WEBSITE_TITLE; %>" />
    <!-- Web Manifest-->
    <link rel="manifest" href="site.webmanifest" />
